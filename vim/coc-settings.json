{
  "coc.preferences.jumpCommand": "vsp",
  "python.linting.enabled": false,
  "python.formatting.provider": "black",
  "coc.preferences.formatOnSave": true,
  "coc.preferences.useQuickfixForLocations": true,
  "coc.preferences.snippets.enable": true,
  "coc.preferences.extensionUpdateCheck": "never",
  "suggest.snippetIndicator": "",
  "coc.source.r-lsp.enable": true,
  "coc.source.r-lsp.filetypes": ["r", "rmarkdown", "vimwiki"],
  "coc.source.rmarkdown.enable": true,
  "coc.source.rmarkdown.filetypes": ["rmarkdown"],
  "coc.source.vimwiki.enable": true,
  "coc.source.vimwiki.filetypes": ["vimwiki"],
  "pyright.filetypes": ["python", "rmarkdown", "vimwiki"],
  "coc-r-lsp.filetypes": ["r", "rmarkdown"]
}
